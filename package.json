    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.17",
    "@types/uuid": "^9.0.6",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
